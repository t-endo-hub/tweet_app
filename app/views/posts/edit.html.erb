<h1>Posts#edit</h1>
<p>Find me in app/views/posts/edit.html.erb</p>
<%= render 'layouts/error_messages', model: @post %>
<%= form_for @post do |f| %>
	<%= f.attachment_field :image %>
	<%= f.label :title, "title" %><br>
	<%= f.text_field :title %><br>
	<%= f.label :body, "body" %><br>
	<%= f.text_area :body %><br>
	<%= f.submit "Update", :disable_with => '送信ing...' %><br>
<% end %>